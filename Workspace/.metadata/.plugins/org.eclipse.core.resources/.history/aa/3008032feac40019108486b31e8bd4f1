package com.hpe.day8;

public class ExceptionEx01 {
	public static void main(String[] args) {
		try {
			int x = 10; 
			int y = 0; 
			
			int z = x/y; 
			
			System.out.println("Result is " + z);

		}catch(Exception e) {
			System.out.println(e);
		}
		
		System.out.println("This is some other business logic ");
	}
}
