package com.hpe.day6;

import java.util.ArrayList;

// type safe 
public class ListEx02 {
	public static void main(String[] args) {
		ArrayList<Integer> list = new ArrayList<Integer>(); 
		
		list.add(10); 
		list.add(20);
		list.add(30);
		list.add(40);
//		list.add(23.4);
		
		System.out.println(list);
		
		
		
	}
}
