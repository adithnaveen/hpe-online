package com.hpe.day2;

public class PersonApp {
	public static void main(String[] args) {
		Person p1 = new Person(); 
		
		p1.pId =100; 
		p1.pName="Magesh"; 
		p1.income=20000; 
		
		
		System.out.println("Person Id " + p1.pId);
		System.out.println("Person Name " + p1.pName);
		System.out.println("Person Income " + p1.income);
	}
}
